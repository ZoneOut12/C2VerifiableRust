{
  "source_file": "./data/frama_c_problems/array_swap.c",
  "function_name": "array_swap",
  "function_signature": "void array_swap(int* arr, int n, int n1, int n2)",
  "preconditions": [
    "n >= 0",
    "0 <= n1 < n && 0 <= n2 < n",
    "\\valid(arr + (0..n-1))"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "arr": [
          1,
          2,
          3
        ],
        "n": 3,
        "n1": 0,
        "n2": 1
      },
      "expected_output": null,
      "description": "Normal swap of first two elements",
      "validation_status": "valid",
      "actual_output": "test_case_1: [2, 1, 3]"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "arr": [
          5,
          6
        ],
        "n": 2,
        "n1": 1,
        "n2": 1
      },
      "expected_output": null,
      "description": "Same indices swap (no change)",
      "validation_status": "valid",
      "actual_output": "test_case_2: [5, 6]"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "arr": [
          10,
          20,
          30,
          40
        ],
        "n": 4,
        "n1": 2,
        "n2": 3
      },
      "expected_output": null,
      "description": "Swap middle elements in larger array",
      "validation_status": "valid",
      "actual_output": "test_case_3: [10, 20, 40, 30]"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "arr": [
          0,
          1,
          2,
          3
        ],
        "n": 4,
        "n1": 0,
        "n2": 3
      },
      "expected_output": null,
      "description": "Swap first and last elements",
      "validation_status": "valid",
      "actual_output": "test_case_4: [3, 1, 2, 0]"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "arr": [
          5,
          4,
          3,
          2,
          1
        ],
        "n": 5,
        "n1": 2,
        "n2": 4
      },
      "expected_output": null,
      "description": "Swap non-adjacent elements",
      "validation_status": "valid",
      "actual_output": "test_case_5: [5, 4, 1, 2, 3]"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "arr": [
          1,
          2,
          3
        ],
        "n": -3,
        "n1": 0,
        "n2": 1
      },
      "expected_output": null,
      "description": "Negative array size violates n >= 0",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
        "arr": [
          1,
          2
        ],
        "n": 3,
        "n1": 2,
        "n2": 0
      },
      "expected_output": null,
      "description": "Array too small for n=3 violates \\valid(arr)",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 8,
      "type": "boundary",
      "inputs": {
        "arr": [
          42
        ],
        "n": 1,
        "n1": 0,
        "n2": 0
      },
      "expected_output": null,
      "description": "Minimum valid array size (1 element)",
      "validation_status": "valid",
      "actual_output": "test_case_8: [42]"
    },
    {
      "test_id": 9,
      "type": "boundary",
      "inputs": {
        "arr": [
          1,
          2
        ],
        "n": 2,
        "n1": 0,
        "n2": 1
      },
      "expected_output": null,
      "description": "Swap first and last in 2-element array",
      "validation_status": "valid",
      "actual_output": "test_case_9: [2, 1]"
    }
  ]
}