{
  "source_file": "./data/frama_c_problems/mult.c",
  "function_name": "mul",
  "function_signature": "int mul(int a, int b)",
  "preconditions": [
    "a >= 0 && b >= 0",
    "a*b <= INT_MAX"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": 2,
        "b": 5
      },
      "expected_output": 10,
      "description": "Normal case with positive integers",
      "validation_status": "valid",
      "actual_output": "10"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": 0,
        "b": 5
      },
      "expected_output": 0,
      "description": "Zero in first parameter",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": 5,
        "b": 0
      },
      "expected_output": 0,
      "description": "Zero in second parameter",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "a": 10,
        "b": 100
      },
      "expected_output": 1000,
      "description": "Larger positive values",
      "validation_status": "valid",
      "actual_output": "1000"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": 1,
        "b": 2147483647
      },
      "expected_output": 2147483647,
      "description": "Product equals INT_MAX",
      "validation_status": "valid",
      "actual_output": "2147483647"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "a": -1,
        "b": 5
      },
      "expected_output": null,
      "description": "Negative first parameter",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
        "a": 1073741824,
        "b": 2
      },
      "expected_output": null,
      "description": "Product exceeds INT_MAX",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 8,
      "type": "boundary",
      "inputs": {
        "a": 0,
        "b": 0
      },
      "expected_output": 0,
      "description": "Both parameters zero",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 9,
      "type": "boundary",
      "inputs": {
        "a": 2147483647,
        "b": 1
      },
      "expected_output": 2147483647,
      "description": "Product at maximum allowed value",
      "validation_status": "valid",
      "actual_output": "2147483647"
    }
  ]
}