{
  "source_file": "./data/frama_c_problems/max.c",
  "function_name": "arraymax",
  "function_signature": "int arraymax(int* a, int n)",
  "preconditions": [
    "\\valid_read(a + (0..n-1))",
    "n > 0"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": [
          1,
          2,
          3,
          4,
          5
        ],
        "n": 5
      },
      "expected_output": 5,
      "description": "Normal increasing array",
      "validation_status": "valid",
      "actual_output": "5"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": [
          5,
          4,
          3,
          2,
          1
        ],
        "n": 5
      },
      "expected_output": 5,
      "description": "Normal decreasing array",
      "validation_status": "valid",
      "actual_output": "5"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": [
          3,
          1,
          4,
          2
        ],
        "n": 4
      },
      "expected_output": 4,
      "description": "Mixed array with max in middle",
      "validation_status": "valid",
      "actual_output": "4"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "a": [
          2,
          2,
          2
        ],
        "n": 3
      },
      "expected_output": 2,
      "description": "All elements same",
      "validation_status": "valid",
      "actual_output": "2"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": [
          42
        ],
        "n": 1
      },
      "expected_output": 42,
      "description": "Single element array",
      "validation_status": "valid",
      "actual_output": "42"
    },
    {
      "test_id": 6,
      "type": "boundary",
      "inputs": {
        "a": [
          9,
          9
        ],
        "n": 2
      },
      "expected_output": 9,
      "description": "Array with all elements same (n=2)",
      "validation_status": "valid",
      "actual_output": "9"
    },
    {
      "test_id": 7,
      "type": "boundary",
      "inputs": {
        "a": [
          7,
          3
        ],
        "n": 2
      },
      "expected_output": 7,
      "description": "Two elements with max first",
      "validation_status": "valid",
      "actual_output": "7"
    },
    {
      "test_id": 8,
      "type": "invalid",
      "inputs": {
        "a": [
          1,
          2,
          3
        ],
        "n": 0
      },
      "expected_output": null,
      "description": "n=0 violates n > 0 precondition",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 9,
      "type": "invalid",
      "inputs": {
        "a": null,
        "n": 3
      },
      "expected_output": null,
      "description": "NULL array pointer violates valid_read precondition",
      "validation_status": "valid",
      "actual_output": null
    }
  ]
}