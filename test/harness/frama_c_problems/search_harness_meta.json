{
  "source_file": "./data/frama_c_problems/search.c",
  "function_name": "arraysearch",
  "function_signature": "int arraysearch(int* a, int x, int n)",
  "preconditions": [
    "n > 0",
    "\\valid_read(a + (0..n-1))"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": [
          5,
          6,
          7
        ],
        "x": 5,
        "n": 3
      },
      "expected_output": 1,
      "description": "x present at first position",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": [
          1,
          2,
          3,
          4
        ],
        "x": 3,
        "n": 4
      },
      "expected_output": 1,
      "description": "x present in middle",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": [
          10,
          20
        ],
        "x": 20,
        "n": 2
      },
      "expected_output": 1,
      "description": "x present at last position",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "a": [
          1,
          2,
          3
        ],
        "x": 5,
        "n": 3
      },
      "expected_output": 0,
      "description": "x not present in array",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": [
          7
        ],
        "x": 7,
        "n": 1
      },
      "expected_output": 1,
      "description": "single element array, x present",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "a": [
          0
        ],
        "x": 5,
        "n": 0
      },
      "expected_output": null,
      "description": "n equals 0 violates n > 0",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
        "a": null,
        "x": 3,
        "n": 2
      },
      "expected_output": null,
      "description": "array pointer is NULL violates valid_read",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 8,
      "type": "boundary",
      "inputs": {
        "a": [
          5
        ],
        "x": 6,
        "n": 1
      },
      "expected_output": 0,
      "description": "single element array, x not present",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 9,
      "type": "boundary",
      "inputs": {
        "a": [
          1,
          2
        ],
        "x": 2,
        "n": 2
      },
      "expected_output": 1,
      "description": "array size 2, x found at last position",
      "validation_status": "valid",
      "actual_output": "1"
    }
  ]
}