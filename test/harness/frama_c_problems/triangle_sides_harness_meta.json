{
  "source_file": "./data/frama_c_problems/triangle_sides.c",
  "function_name": "validts",
  "function_signature": "int validts(int a, int b, int c)",
  "preconditions": [
    "a>0 && b>0 && c>0",
    "a+b+c <= INT_MAX"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": 2,
        "b": 3,
        "c": 4
      },
      "expected_output": 1,
      "description": "Valid triangle with sides 2,3,4",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": 5,
        "b": 5,
        "c": 5
      },
      "expected_output": 1,
      "description": "Equilateral triangle with sides 5,5,5",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": 3,
        "b": 4,
        "c": 5
      },
      "expected_output": 1,
      "description": "Right-angled triangle with sides 3,4,5",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "a": 10,
        "b": 10,
        "c": 5
      },
      "expected_output": 1,
      "description": "Isosceles triangle with sides 10,10,5",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": 7,
        "b": 10,
        "c": 15
      },
      "expected_output": 1,
      "description": "Scalene triangle with sides 7,10,15",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "a": 0,
        "b": 3,
        "c": 4
      },
      "expected_output": null,
      "description": "Invalid input: a=0 violates a>0",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
        "a": 2147483646,
        "b": 1,
        "c": 1
      },
      "expected_output": null,
      "description": "Invalid input: sum exceeds INT_MAX (a=INT_MAX-1, b=1, c=1)",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 8,
      "type": "boundary",
      "inputs": {
        "a": 1,
        "b": 1,
        "c": 1
      },
      "expected_output": 1,
      "description": "Minimal valid triangle with sides 1,1,1",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 9,
      "type": "boundary",
      "inputs": {
        "a": 2147483645,
        "b": 1,
        "c": 1
      },
      "expected_output": "0",
      "description": "Valid triangle with sum exactly INT_MAX (a=INT_MAX-2, b=1, c=1)",
      "validation_status": "valid",
      "actual_output": "0"
    }
  ]
}