{
  "source_file": "./data/frama_c_problems/array_find.c",
  "function_name": "array_find",
  "function_signature": "int array_find(int* arr, int n, int x)",
  "preconditions": [
    "n >= 0",
    "\\valid_read(arr+(0..n-1))"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "arr": [
          10,
          20,
          30,
          40
        ],
        "n": 4,
        "x": 30
      },
      "expected_output": 2,
      "description": "x found in the middle of the array",
      "validation_status": "valid",
      "actual_output": "2"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "arr": [
          5,
          15,
          25
        ],
        "n": 3,
        "x": 5
      },
      "expected_output": 0,
      "description": "x is the first element",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "arr": [
          1,
          3,
          5
        ],
        "n": 3,
        "x": 5
      },
      "expected_output": 2,
      "description": "x is the last element",
      "validation_status": "valid",
      "actual_output": "2"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "arr": [
          2,
          4,
          6,
          8
        ],
        "n": 4,
        "x": 5
      },
      "expected_output": -1,
      "description": "x not found in array",
      "validation_status": "valid",
      "actual_output": "-1"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "arr": [
          42
        ],
        "n": 1,
        "x": 42
      },
      "expected_output": 0,
      "description": "x found in single-element array",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 6,
      "type": "boundary",
      "inputs": {
        "arr": [
          0
        ],
        "n": 0,
        "x": 99
      },
      "expected_output": -1,
      "description": "empty array (n=0)",
      "validation_status": "valid",
      "actual_output": "-1"
    },
    {
      "test_id": 7,
      "type": "boundary",
      "inputs": {
        "arr": [
          77
        ],
        "n": 1,
        "x": 0
      },
      "expected_output": -1,
      "description": "single-element array with x not found",
      "validation_status": "valid",
      "actual_output": "-1"
    },
    {
      "test_id": 8,
      "type": "invalid",
      "inputs": {
        "arr": [
          1,
          2,
          3
        ],
        "n": -3,
        "x": 0
      },
      "expected_output": null,
      "description": "negative n violates n >= 0",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 9,
      "type": "invalid",
      "inputs": {
        "arr": null,
        "n": 2,
        "x": 5
      },
      "expected_output": null,
      "description": "NULL array pointer violates \\valid_read",
      "validation_status": "valid",
      "actual_output": null
    }
  ]
}