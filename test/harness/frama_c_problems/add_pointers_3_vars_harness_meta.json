{
  "source_file": "./data/frama_c_problems/add_pointers_3_vars.c",
  "function_name": "add",
  "function_signature": "int add(int *a, int *b, int *r)",
  "preconditions": [
    "\\valid_read(a) && \\valid_read(b) && \\valid_read(r)",
    "\\separated(a, b, r)",
    "*a + *b <= INT_MAX",
    "*a + *b >= INT_MIN",
    "*a + *b + *r <= INT_MAX",
    "*a + *b + *r >= INT_MIN"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": 24,
        "b": 32,
        "r": 12
      },
      "expected_output": 68,
      "description": "Normal case with positive values",
      "validation_status": "valid",
      "actual_output": "68"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": 2147483645,
        "b": 1,
        "r": 1
      },
      "expected_output": 2147483647,
      "description": "Sum a+b reaches INT_MAX-1, total sum INT_MAX",
      "validation_status": "valid",
      "actual_output": "2147483647"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": -10,
        "b": -20,
        "r": -30
      },
      "expected_output": -60,
      "description": "All negative values",
      "validation_status": "valid",
      "actual_output": "-60"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "a": 0,
        "b": 0,
        "r": 0
      },
      "expected_output": 0,
      "description": "All zeros",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": -1073741824,
        "b": -1073741824,
        "r": 1
      },
      "expected_output": -2147483647,
      "description": "Sum a+b is INT_MIN, adding r=1",
      "validation_status": "valid",
      "actual_output": "-2147483647"
    },
    {
      "test_id": 6,
      "type": "boundary",
      "inputs": {
        "a": 2147483647,
        "b": 0,
        "r": 0
      },
      "expected_output": 2147483647,
      "description": "Sum at INT_MAX",
      "validation_status": "valid",
      "actual_output": "2147483647"
    },
    {
      "test_id": 7,
      "type": "boundary",
      "inputs": {
        "a": -2147483648,
        "b": 0,
        "r": 0
      },
      "expected_output": -2147483648,
      "description": "Sum at INT_MIN",
      "validation_status": "valid",
      "actual_output": "-2147483648"
    },
    {
      "test_id": 8,
      "type": "invalid",
      "inputs": {
        "a": 5,
        "b": 5,
        "r": 0
      },
      "expected_output": null,
      "description": "a and b point to same variable, violating separated",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 9,
      "type": "invalid",
      "inputs": {
        "a": 2147483647,
        "b": 1,
        "r": 0
      },
      "expected_output": null,
      "description": "a + b overflows INT_MAX",
      "validation_status": "valid",
      "actual_output": null
    }
