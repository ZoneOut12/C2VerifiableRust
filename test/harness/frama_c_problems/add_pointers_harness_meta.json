{
  "source_file": "./data/frama_c_problems/add_pointers.c",
  "function_name": "add",
  "function_signature": "int add(int *p, int *q)",
  "preconditions": [
    "\\valid_read(p) && \\valid_read(q)",
    "\\separated(p, q)",
    "*p + *q <= INT_MAX",
    "*p + *q >= INT_MIN"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "p": [
          24
        ],
        "q": [
          32
        ]
      },
      "expected_output": 56,
      "description": "Valid case with positive numbers",
      "validation_status": "valid",
      "actual_output": "56"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "p": [
          -10
        ],
        "q": [
          -20
        ]
      },
      "expected_output": -30,
      "description": "Valid case with negative numbers",
      "validation_status": "valid",
      "actual_output": "-30"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "p": [
          50
        ],
        "q": [
          -20
        ]
      },
      "expected_output": 30,
      "description": "Valid case with mixed signs",
      "validation_status": "valid",
      "actual_output": "30"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "p": [
          100
        ],
        "q": [
          0
        ]
      },
      "expected_output": 100,
      "description": "Valid case with zero",
      "validation_status": "valid",
      "actual_output": "100"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "p": [
          0
        ],
        "q": [
          0
        ]
      },
      "expected_output": 0,
      "description": "Valid case with zeros",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "p": [
          100
        ],
        "q": [
          100
        ]
      },
      "expected_output": null,
      "description": "Invalid case with overlapping pointers",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
