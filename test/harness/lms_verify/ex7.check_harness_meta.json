      "type": "valid",
      "inputs": {
        "x0": [
          5,
          6,
          7
        ],
        "x1": 3,
        "x2": 4
      },
      "expected_output": -1,
      "description": "Valid - element not found",
      "validation_status": "valid",
      "actual_output": "-1"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "x0": [
          10,
          20,
          30
        ],
        "x1": 3,
        "x2": 10
