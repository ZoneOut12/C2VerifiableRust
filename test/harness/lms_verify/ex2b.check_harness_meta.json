{
  "source_file": "./data/lms_verify/ex2b.check.c",
  "function_name": "pick_first_element",
  "function_signature": "int pick_first_element(int *x6, int x7)",
  "preconditions": [
    "(x7>0)",
    "\\valid(x6+(0..x7-1))"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "x6": [
          10,
          20,
          30,
          40,
          50
        ],
        "x7": 5
      },
      "expected_output": 10,
      "description": "Valid case with x7=5 and array of 5 elements",
      "validation_status": "valid",
      "actual_output": "10"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "x6": [
          42
        ],
        "x7": 1
      },
      "expected_output": 42,
      "description": "Valid case with x7=1 and single-element array",
      "validation_status": "valid",
      "actual_output": "42"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "x6": [
          5,
          6
        ],
        "x7": 2
      },
      "expected_output": 5,
      "description": "Valid case with x7=2 and two-element array",
      "validation_status": "valid",
      "actual_output": "5"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "x6": [
          0,
          -1,
          4
        ],
        "x7": 3
      },
      "expected_output": 0,
      "description": "Valid case with negative numbers and zeros",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "x6": [
          100,
          200,
          300,
          400
        ],
        "x7": 4
      },
      "expected_output": 100,
      "description": "Valid case with x7=4 and four-element array",
      "validation_status": "valid",
      "actual_output": "100"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "x6": [
          1,
          2,
          3
        ],
        "x7": 0
      },
      "expected_output": null,
      "description": "Invalid x7=0 which violates x7>0 precondition",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
        "x6": [
          5
        ],
        "x7": 2
      },
      "expected_output": null,
      "description": "Invalid array size (only 1 element when x7=2)",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 8,
      "type": "boundary",
      "inputs": {
        "x6": [
          99
        ],
        "x7": 1
      },
      "expected_output": 99,
      "description": "Boundary case with minimum allowed x7 (1)",
      "validation_status": "valid",
      "actual_output": "99"
    },
    {
      "test_id": 9,
      "type": "boundary",
      "inputs": {
        "x6": [
          15,
          25
        ],
        "x7": 2
      },
      "expected_output": 15,
      "description": "Boundary case with x7=2 and two elements",
      "validation_status": "valid",
      "actual_output": "15"
    }
  ]
}