{
  "source_file": "./data/lms_verify/lina2.c",
  "function_name": "index_where",
  "function_signature": "int index_where(int* v, int n, int* o)",
  "preconditions": [
    "n > 0",
    "\\valid(v+(0..n-1))",
    "\\valid(o+(0..n-1))"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "v": [
          1,
          2,
          3,
          4,
          5
        ],
        "n": 5,
        "o": [
          0,
          0,
          0,
          0,
          0
        ]
      },
      "expected_output": 2,
      "description": "Normal case with even numbers at indices 1 and 3",
      "validation_status": "valid",
      "actual_output": "2"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "v": [
          2,
          4,
          6
        ],
        "n": 3,
        "o": [
          0,
          0,
          0
        ]
      },
      "expected_output": 3,
      "description": "All elements even, output array filled completely",
      "validation_status": "valid",
      "actual_output": "3"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "v": [
          1,
          3,
          5
        ],
        "n": 3,
        "o": [
          0,
          0,
          0
        ]
      },
      "expected_output": 0,
      "description": "No even numbers, output array empty",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "v": [
          2,
          3,
          4
        ],
        "n": 3,
        "o": [
          0,
          0,
          0
        ]
      },
      "expected_output": 2,
      "description": "Even numbers at first and third positions",
      "validation_status": "valid",
      "actual_output": "2"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "v": [
          2
        ],
        "n": 1,
        "o": [
          0
        ]
      },
      "expected_output": 1,
      "description": "Single even element",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 6,
      "type": "boundary",
      "inputs": {
        "v": [
          3
        ],
        "n": 1,
        "o": [
          0
        ]
      },
      "expected_output": 0,
      "description": "Single odd element",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 7,
      "type": "boundary",
      "inputs": {
        "v": [
          2,
          4
        ],
        "n": 2,
        "o": [
          0,
          0
        ]
      },
      "expected_output": 2,
      "description": "All even elements with output array filled completely",
      "validation_status": "valid",
      "actual_output": "2"
    },
    {
      "test_id": 8,
      "type": "invalid",
      "inputs": {
        "v": [
          1,
          2,
          3
        ],
        "n": 0,
        "o": [
          0,
          0,
          0
        ]
      },
      "expected_output": null,
      "description": "n is zero violating n > 0 precondition",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 9,
      "type": "invalid",
      "inputs": {
        "v": null,
        "n": 2,
        "o": [
          0,
          0
        ]
      },
      "expected_output": null,
      "description": "v is NULL pointer violating array validity",
      "validation_status": "valid",
      "actual_output": null
    }
  ]
}