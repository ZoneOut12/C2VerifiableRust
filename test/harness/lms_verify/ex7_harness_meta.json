{
  "source_file": "./data/lms_verify/ex7.c",
  "function_name": "member",
  "function_signature": "int member(int* p, int n, int v)",
  "preconditions": [
    "n>0",
    "\\valid(p+(0..n-1))"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "p": [
          5,
          2,
          3
        ],
        "n": 3,
        "v": 5
      },
      "expected_output": 0,
      "description": "Element found at first position",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "p": [
          1,
          3,
          5,
          7,
          9
        ],
        "n": 5,
        "v": 5
      },
      "expected_output": 2,
      "description": "Element found in middle",
      "validation_status": "valid",
      "actual_output": "2"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "p": [
          10,
          20,
          30,
          40
        ],
        "n": 4,
        "v": 15
      },
      "expected_output": -1,
      "description": "Element not found",
      "validation_status": "valid",
      "actual_output": "-1"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "p": [
          10,
          20,
          30,
          40
        ],
        "n": 4,
        "v": 40
      },
      "expected_output": 3,
      "description": "Element found at last position",
      "validation_status": "valid",
      "actual_output": "3"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "p": [
          2,
          2,
          2,
          2,
          2
        ],
        "n": 5,
        "v": 2
      },
      "expected_output": 0,
      "description": "Multiple occurrences, returns first",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "p": [
          1,
          2,
          3
        ],
        "n": 0,
        "v": 5
      },
      "expected_output": null,
      "description": "Invalid n=0 (violates n>0)",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
        "p": null,
        "n": 3,
        "v": 5
      },
      "expected_output": null,
      "description": "Invalid NULL array (violates \\valid)",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 8,
      "type": "boundary",
      "inputs": {
        "p": [
          42
        ],
        "n": 1,
        "v": 42
      },
      "expected_output": 0,
      "description": "n=1 with element present",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 9,
      "type": "boundary",
      "inputs": {
        "p": [
          42
        ],
        "n": 1,
        "v": 0
      },
      "expected_output": -1,
      "description": "n=1 with element not present",
      "validation_status": "valid",
      "actual_output": "-1"
    }
  ]
}