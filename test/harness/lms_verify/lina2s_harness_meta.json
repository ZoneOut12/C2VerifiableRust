{
  "source_file": "./data/lms_verify/lina2s.c",
  "function_name": "index_where",
  "function_signature": "void index_where(struct vector* a, struct vector* o)",
  "preconditions": [
    "\\valid(a)",
    "\\valid(o)",
    "\\valid(a->v+(0..a->n-1))",
    "\\valid(o->v+(0..a->n-1))",
    "\\forall int i; \\forall int j; 0 <= i < a->n && 0 <= j < a->n ==> \\separated(a, o, a->v+i, o->v+j)",
    "a->n > 0"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": {
          "n": 3,
          "v": [
            2,
            4,
            6
          ]
        },
        "o": {
          "n": 3,
          "v": [
            0,
            0,
            0
          ]
        }
      },
      "expected_output": "test_case_1: o.n=3, o.v[0]=0, o.v[1]=1, o.v[2]=2",
      "description": "All elements match predicate (even numbers)",
      "validation_status": "valid",
      "actual_output": "test_case_1: o.n=3, o.v[0]=0, o.v[1]=1, o.v[2]=2"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": {
          "n": 3,
          "v": [
            1,
            2,
            3
          ]
        },
        "o": {
          "n": 3,
          "v": [
            0,
            0,
            0
          ]
        }
      },
      "expected_output": "test_case_2: o.n=1, o.v[0]=1",
      "description": "Mixed even and odd elements",
      "validation_status": "valid",
      "actual_output": "test_case_2: o.n=1, o.v[0]=1"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": {
          "n": 3,
          "v": [
