      "type": "boundary",
      "inputs": {
        "x": -2147483648
      },
      "expected_output": -2147483647,
      "description": "Boundary input: minimum integer value (INT_MIN)",
      "validation_status": "valid",
      "actual_output": "-2147483647"
    },
