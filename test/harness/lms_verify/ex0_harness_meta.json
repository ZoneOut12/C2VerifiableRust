      "type": "valid",
      "inputs": {
        "x": -10
      },
      "expected_output": -9,
      "description": "Valid input: negative number",
      "validation_status": "valid",
      "actual_output": "-9"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "x": 1000
      },
      "expected_output": 1001,
      "description": "Valid input: mid-range positive",
      "validation_status": "valid",
      "actual_output": "1001"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "x": 2147483645
      },
      "expected_output": 2147483646,
