{
  "source_file": "./data/tutorial_wp/ex-1-past-answer.c",
  "function_name": "distance",
  "function_signature": "int distance(int a, int b)",
  "preconditions": [
    "a < b  ==> b - a <= INT_MAX",
    "b <= a ==> a - b <= INT_MAX"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": 5,
        "b": 10
      },
      "expected_output": 5,
      "description": "Normal case with a < b",
      "validation_status": "valid",
      "actual_output": "5"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": 0,
        "b": 0
      },
      "expected_output": 0,
      "description": "Equal values",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": -5,
        "b": 3
      },
      "expected_output": 8,
      "description": "Negative a and positive b",
      "validation_status": "valid",
      "actual_output": "8"
    },
    {
      "test_id": 4,
      "type": "boundary",
      "inputs": {
        "a": "INT_MAX-1",
        "b": "INT_MAX"
      },
      "expected_output": 1,
      "description": "Maximum allowed difference in positive range",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": "INT_MIN+1",
        "b": "INT_MIN+2"
      },
      "expected_output": 1,
      "description": "Two increments above minimum int",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "a": "INT_MIN",
        "b": "INT_MAX"
      },
      "expected_output": null,
      "description": "a < b but difference exceeds INT_MAX",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
        "a": "INT_MAX",
        "b": "INT_MIN"
      },
      "expected_output": null,
      "description": "b <= a but difference exceeds INT_MAX",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 8,
      "type": "boundary",
      "inputs": {
        "a": 0,
        "b": "INT_MAX"
      },
      "expected_output": "2147483647",
      "description": "Maximum allowed difference with zero",
      "validation_status": "valid",
      "actual_output": "2147483647"
    },
    {
      "test_id": 9,
      "type": "boundary",
      "inputs": {
        "a": "INT_MIN",
        "b": "INT_MIN"
      },
      "expected_output": 0,
      "description": "Minimum int values",
      "validation_status": "valid",
      "actual_output": "0"
    }
  ]
}