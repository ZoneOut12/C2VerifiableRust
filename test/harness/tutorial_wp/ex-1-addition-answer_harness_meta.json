{
  "source_file": "./data/tutorial_wp/ex-1-addition-answer.c",
  "function_name": "add",
  "function_signature": "int add(int x, int y)",
  "preconditions": [
    "INT_MIN <= x+y <= INT_MAX"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "x": 5,
        "y": 10
      },
      "expected_output": 15,
      "description": "Normal positive numbers",
      "validation_status": "valid",
      "actual_output": "15"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "x": 100,
        "y": 200
      },
      "expected_output": 300,
      "description": "Larger positive numbers",
      "validation_status": "valid",
      "actual_output": "300"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "x": -5,
        "y": -10
      },
      "expected_output": -15,
      "description": "Negative numbers",
      "validation_status": "valid",
      "actual_output": "-15"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "x": 2147483646,
        "y": 1
      },
      "expected_output": 2147483647,
      "description": "Sum at INT_MAX (32-bit)",
      "validation_status": "valid",
      "actual_output": "2147483647"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "x": -2147483647,
        "y": -1
      },
      "expected_output": -2147483648,
      "description": "Sum at INT_MIN (32-bit)",
      "validation_status": "valid",
      "actual_output": "-2147483648"
    },
    {
      "test_id": 6,
      "type": "boundary",
      "inputs": {
        "x": 2147483647,
        "y": 0
      },
      "expected_output": 2147483647,
      "description": "x at INT_MAX, y at boundary",
      "validation_status": "valid",
      "actual_output": "2147483647"
    },
    {
      "test_id": 7,
      "type": "boundary",
      "inputs": {
        "x": -2147483648,
        "y": 0
      },
      "expected_output": -2147483648,
      "description": "x at INT_MIN, y at boundary",
      "validation_status": "valid",
      "actual_output": "-2147483648"
    },
    {
      "test_id": 8,
      "type": "invalid",
      "inputs": {
        "x": 2147483647,
        "y": 1
      },
      "expected_output": null,
      "description": "Sum exceeds INT_MAX (overflow)",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 9,
      "type": "invalid",
      "inputs": {
        "x": -2147483648,
        "y": -1
      },
      "expected_output": null,
      "description": "Sum below INT_MIN (underflow)",
      "validation_status": "valid",
      "actual_output": null
    }
  ]
}