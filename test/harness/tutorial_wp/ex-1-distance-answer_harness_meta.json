{
  "source_file": "./data/tutorial_wp/ex-1-distance-answer.c",
  "function_name": "distance",
  "function_signature": "int distance(int a, int b)",
  "preconditions": [
    "a < b  ==> b - a <= INT_MAX",
    "b <= a ==> a - b <= INT_MAX"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": 5,
        "b": 10
      },
      "expected_output": 5,
      "description": "Normal case where a < b, difference 5",
      "validation_status": "valid",
      "actual_output": "5"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": -3,
        "b": 4
      },
      "expected_output": 7,
      "description": "a < b with negative a and positive b",
      "validation_status": "valid",
      "actual_output": "7"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": 0,
        "b": 0
      },
      "expected_output": 0,
      "description": "Equal values, returns zero",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "a": 10,
        "b": 3
      },
      "expected_output": 7,
      "description": "a > b, returns difference",
      "validation_status": "valid",
      "actual_output": "7"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": -2147483647,
        "b": -2147483648
      },
      "expected_output": 1,
      "description": "a just above INT_MIN, returns 1",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 6,
      "type": "boundary",
      "inputs": {
        "a": 0,
        "b": 2147483647
      },
      "expected_output": 2147483647,
      "description": "Maximum allowed difference when b > a",
      "validation_status": "valid",
      "actual_output": "2147483647"
    },
    {
      "test_id": 7,
      "type": "boundary",
      "inputs": {
        "a": -2147483648,
        "b": -1
      },
      "expected_output": 2147483647,
      "description": "Maximum allowed difference when a < b",
      "validation_status": "valid",
      "actual_output": "2147483647"
    },
    {
      "test_id": 8,
      "type": "invalid",
      "inputs": {
        "a": -2147483648,
        "b": 0
      },
      "expected_output": null,
      "description": "a < b and b - a exceeds INT_MAX (INT_MIN to 0)",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 9,
      "type": "invalid",
      "inputs": {
        "a": 2147483647,
        "b": -1
      },
      "expected_output": null,
      "description": "a >= b and a - b exceeds INT_MAX (INT_MAX minus -1)",
      "validation_status": "valid",
      "actual_output": null
    }
  ]
}