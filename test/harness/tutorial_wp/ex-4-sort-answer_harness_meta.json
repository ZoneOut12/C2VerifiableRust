{
  "source_file": "./data/tutorial_wp/ex-4-sort-answer.c",
  "function_name": "sort",
  "function_signature": "void sort(int* a, size_t beg, size_t end)",
  "preconditions": [
    "beg <= end",
    "\\valid(a+(beg .. end-1))"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": [
          5,
          3,
          2,
          4
        ],
        "beg": 0,
        "end": 4
      },
      "expected_output": null,
      "description": "Valid - normal array [5,3,2,4] sorted to [2,3,4,5]",
      "validation_status": "valid",
      "actual_output": "test_case_1: [2, 3, 4, 5]"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": [
          42
        ],
        "beg": 0,
        "end": 1
      },
      "expected_output": null,
      "description": "Valid - single element array [42] remains unchanged",
      "validation_status": "valid",
      "actual_output": "test_case_2: [42]"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": [
          1,
          2,
          3,
          4
        ],
        "beg": 0,
        "end": 4
      },
      "expected_output": null,
      "description": "Valid - already sorted array [1,2,3,4] remains unchanged",
      "validation_status": "valid",
      "actual_output": "test_case_3: [1, 2, 3, 4]"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "a": [
          4,
          3,
          2,
          1
        ],
        "beg": 0,
        "end": 4
      },
      "expected_output": null,
      "description": "Valid - descending array [4,3,2,1] sorted to [1,2,3,4]",
      "validation_status": "valid",
      "actual_output": "test_case_4: [1, 2, 3, 4]"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": [
          3,
          2,
          3,
          2
        ],
        "beg": 0,
        "end": 4
      },
      "expected_output": null,
      "description": "Valid - array with duplicates [3,2,3,2] sorted to [2,2,3,3]",
      "validation_status": "valid",
      "actual_output": "test_case_5: [2, 2, 3, 3]"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "a": [
          1,
          2,
          3
        ],
        "beg": 3,
        "end": 2
      },
      "expected_output": null,
      "description": "Invalid - beg > end violates precondition",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
        "a": [
          1,
          2
        ],
        "beg": 0,
        "end": 3
      },
      "expected_output": null,
      "description": "Invalid - array too small for end=3",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 8,
      "type": "boundary",
      "inputs": {
        "a": [
          1,
          2,
          3,
          4
        ],
        "beg": 2,
        "end": 2
      },
      "expected_output": null,
      "description": "Boundary - empty range (beg == end), array unchanged",
      "validation_status": "valid",
      "actual_output": "test_case_8: [1, 2, 3, 4]"
    },
    {
      "test_id": 9,
      "type": "boundary",
      "inputs": {
        "a": [
          42
        ],
        "beg": 0,
        "end": 1
      },
      "expected_output": null,
      "description": "Boundary - single element range, array unchanged",
      "validation_status": "valid",
      "actual_output": "test_case_9: [42]"
    }
  ]
}