{
  "source_file": "./data/tutorial_wp/search-1.c",
  "function_name": "search",
  "function_signature": "int* search(int* array, size_t length, int element)",
  "preconditions": [
    "\\valid_read(array + (0 .. length-1))"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "array": [
          1,
          2,
          3,
          4
        ],
        "length": 4,
        "element": 3
      },
      "expected_output": 3,
      "description": "Element found in the middle of the array",
      "validation_status": "valid",
      "actual_output": "3"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "array": [
          5,
          6,
          7
        ],
        "length": 3,
        "element": 5
      },
      "expected_output": 5,
      "description": "Element found at the start of the array",
      "validation_status": "valid",
      "actual_output": "5"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "array": [
          10,
          20
        ],
        "length": 2,
        "element": 20
      },
      "expected_output": 20,
      "description": "Element found at the end of the array",
      "validation_status": "valid",
      "actual_output": "20"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "array": [
          42
        ],
        "length": 1,
        "element": 99
      },
      "expected_output": null,
      "description": "Element not found in single-element array",
      "validation_status": "valid",
      "actual_output": "test_case_4: NULL"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "array": [
          1,
          3,
          5
        ],
        "length": 3,
        "element": 2
      },
      "expected_output": null,
      "description": "Element not found in multi-element array",
      "validation_status": "valid",
      "actual_output": "test_case_5: NULL"
    },
    {
      "test_id": 6,
      "type": "boundary",
      "inputs": {
        "array": [],
        "length": 0,
        "element": 5
      },
      "expected_output": null,
      "description": "Empty array (length 0)",
      "validation_status": "valid",
      "actual_output": "test_case_6: NULL"
    },
    {
      "test_id": 7,
      "type": "boundary",
      "inputs": {
        "array": [
          7,
          8,
          9
        ],
        "length": 3,
        "element": 9
      },
      "expected_output": 9,
      "description": "Element found at the last position",
      "validation_status": "valid",
      "actual_output": "9"
    },
    {
      "test_id": 8,
      "type": "invalid",
      "inputs": {
        "array": null,
        "length": 5,
        "element": 3
      },
      "expected_output": null,
      "description": "NULL array pointer (violates precondition)",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 9,
      "type": "invalid",
      "inputs": {
        "array": [
          1,
          2
        ],
        "length": 3,
        "element": 0
      },
      "expected_output": null,
      "description": "Array length smaller than specified (buffer overflow)",
      "validation_status": "valid",
      "actual_output": null
    }
  ]
}