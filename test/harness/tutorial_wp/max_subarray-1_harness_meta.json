{
  "source_file": "./data/tutorial_wp/max_subarray-1.c",
  "function_name": "max_subarray",
  "function_signature": "int max_subarray(int *a, size_t len)",
  "preconditions": [
    "\\valid(a+(0..len-1))",
    "\\forall integer i, j; INT_MIN <= sum(a,i,j) <= INT_MAX"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": [
          1,
          2,
          3
        ],
        "len": 3
      },
      "expected_output": 6,
      "description": "Normal array with positive elements",
      "validation_status": "valid",
      "actual_output": "6"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": [
          2,
          -1,
          3,
          -2
        ],
        "len": 4
      },
      "expected_output": 4,
      "description": "Mixed positive and negative values",
      "validation_status": "valid",
      "actual_output": "4"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": [
          -5,
          -3,
          -2
        ],
        "len": 3
      },
      "expected_output": 0,
      "description": "All negative values (empty subarray optimal)",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "a": [
          42
        ],
        "len": 1
      },
      "expected_output": 42,
      "description": "Single positive element",
      "validation_status": "valid",
      "actual_output": "42"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": [
          0
        ],
        "len": 1
      },
      "expected_output": 0,
      "description": "Single zero element",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "a": null,
        "len": 3
      },
      "expected_output": null,
      "description": "NULL array pointer violates validity",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 7,
      "type": "invalid",
      "inputs": {
        "a": [
          1073741824,
          1073741824
        ],
        "len": 2
      },
      "expected_output": null,
      "description": "Subarray sum exceeds INT_MAX (2^30 + 2^30 = 2^31 > INT_MAX)",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 8,
      "type": "boundary",
      "inputs": {
        "a": [],
        "len": 0
      },
      "expected_output": 0,
      "description": "Empty array (valid edge case)",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 9,
      "type": "boundary",
      "inputs": {
        "a": [
          2147483647
        ],
        "len": 1
      },
      "expected_output": 2147483647,
      "description": "Single element at INT_MAX",
      "validation_status": "valid",
      "actual_output": "2147483647"
    }
  ]
}