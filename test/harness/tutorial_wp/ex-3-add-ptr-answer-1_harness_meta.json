{
  "source_file": "./data/tutorial_wp/ex-3-add-ptr-answer-1.c",
  "function_name": "add",
  "function_signature": "int add(int *p, int *q)",
  "preconditions": [
    "INT_MIN <= *p + *q <= INT_MAX",
    "\\valid_read(p) && \\valid_read(q)"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "p": 24,
        "q": 42
      },
      "expected_output": 66,
      "description": "Normal case with distinct pointers",
      "validation_status": "valid",
      "actual_output": "66"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "p": 24,
        "q": 24
      },
      "expected_output": 48,
      "description": "Same pointer for both arguments",
      "validation_status": "valid",
      "actual_output": "48"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "p": 100,
        "q": 200
      },
      "expected_output": 300,
      "description": "Two positive numbers",
      "validation_status": "valid",
      "actual_output": "300"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "p": -5,
        "q": 10
      },
      "expected_output": 5,
      "description": "Positive and negative sum to positive",
      "validation_status": "valid",
      "actual_output": "5"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "p": 0,
        "q": 0
      },
      "expected_output": 0,
      "description": "Zero values",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 6,
      "type": "boundary",
      "inputs": {
        "p": 2147483647,
        "q": 0
      },
      "expected_output": 2147483647,
      "description": "Sum equals INT_MAX",
      "validation_status": "valid",
      "actual_output": "2147483647"
    },
    {
      "test_id": 7,
      "type": "boundary",
      "inputs": {
        "p": -2147483648,
        "q": 0
      },
      "expected_output": -2147483648,
      "description": "Sum equals INT_MIN",
      "validation_status": "valid",
      "actual_output": "-2147483648"
    },
    {
      "test_id": 8,
      "type": "invalid",
      "inputs": {
        "p": null,
        "q": 5
      },
      "expected_output": null,
      "description": "p is NULL pointer",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 9,
      "type": "invalid",
      "inputs": {
        "p": 2147483647,
        "q": 1
      },
      "expected_output": null,
      "description": "Sum exceeds INT_MAX",
      "validation_status": "valid",
      "actual_output": null
    }
  ]
}