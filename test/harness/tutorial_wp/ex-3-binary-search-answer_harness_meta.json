{
  "source_file": "./data/tutorial_wp/ex-3-binary-search-answer.c",
  "function_name": "bsearch",
  "function_signature": "int bsearch(int* arr, int len, int value)",
  "preconditions": [
    "\\valid_read(arr + (0 .. len-1))",
    "\\forall integer i, j ; 0 <= i <= j < len ==> arr[i] <= arr[j]",
    "len >= 0"
  ],
  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "arr": [
          1,
          2,
          3,
          4,
          5
        ],
        "len": 5,
        "value": 3
      },
      "expected_output": 2,
      "description": "Value in middle of sorted array",
      "validation_status": "valid",
      "actual_output": "2"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "arr": [
          10,
          20,
          30
        ],
        "len": 3,
        "value": 10
      },
      "expected_output": 0,
      "description": "Value at first position",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "arr": [
          5,
          6,
          7,
          8
        ],
        "len": 4,
        "value": 8
      },
      "expected_output": 3,
      "description": "Value at last position",
      "validation_status": "valid",
      "actual_output": "3"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "arr": [
          42
        ],
        "len": 1,
        "value": 42
      },
      "expected_output": 0,
      "description": "Single element array, value found",
      "validation_status": "valid",
      "actual_output": "0"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "arr": [
          42
        ],
        "len": 1,
        "value": 0
      },
      "expected_output": -1,
      "description": "Single element array, value not found",
      "validation_status": "valid",
      "actual_output": "-1"
    },
    {
      "test_id": 6,
      "type": "boundary",
      "inputs": {
        "arr": [],
        "len": 0,
        "value": 5
      },
      "expected_output": -1,
      "description": "Empty array",
      "validation_status": "valid",
      "actual_output": "-1"
    },
    {
      "test_id": 7,
      "type": "boundary",
      "inputs": {
        "arr": [
          2,
          2,
          2,
          2
        ],
        "len": 4,
        "value": 2
      },
      "expected_output": 1,
      "description": "All elements equal, value present",
      "validation_status": "valid",
      "actual_output": "1"
    },
    {
      "test_id": 8,
      "type": "invalid",
      "inputs": {
        "arr": [
          1,
          2,
          3
        ],
        "len": -1,
        "value": 0
      },
      "expected_output": null,
      "description": "Negative length violates precondition",
      "validation_status": "valid",
      "actual_output": null
    },
    {
      "test_id": 9,
      "type": "invalid",
      "inputs": {
        "arr": [
          1,
          3,
          2,
          4
        ],
        "len": 4,
        "value": 2
      },
      "expected_output": null,
      "description": "Unsorted array violates sorted precondition",
      "validation_status": "valid",
      "actual_output": null
    }
  ]
}