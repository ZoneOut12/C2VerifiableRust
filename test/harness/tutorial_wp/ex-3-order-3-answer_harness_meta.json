  "test_cases": [
    {
      "test_id": 1,
      "type": "valid",
      "inputs": {
        "a": 3,
        "b": 5
      },
      "expected_output": "test_case_1: a=5, b=3",
      "description": "a < b, values swapped",
      "validation_status": "valid",
      "actual_output": "test_case_1: a=5, b=3"
    },
    {
      "test_id": 2,
      "type": "valid",
      "inputs": {
        "a": 5,
        "b": 3
      },
      "expected_output": "test_case_2: a=5, b=3",
      "description": "a > b, no swap",
      "validation_status": "valid",
      "actual_output": "test_case_2: a=5, b=3"
    },
    {
      "test_id": 3,
      "type": "valid",
      "inputs": {
        "a": 4,
        "b": 4
      },
      "expected_output": "test_case_3: a=4, b=4",
      "description": "a equals b, no swap",
      "validation_status": "valid",
      "actual_output": "test_case_3: a=4, b=4"
    },
    {
      "test_id": 4,
      "type": "valid",
      "inputs": {
        "a": -5,
        "b": -3
      },
      "expected_output": "test_case_4: a=-3, b=-5",
      "description": "Negative values, a < b, swapped",
      "validation_status": "valid",
      "actual_output": "test_case_4: a=-3, b=-5"
    },
    {
      "test_id": 5,
      "type": "valid",
      "inputs": {
        "a": -2147483648,
        "b": 2147483647
      },
      "expected_output": "test_case_5: a=2147483647, b=-2147483648",
      "description": "a is INT_MIN and b is INT_MAX, swapped",
      "validation_status": "valid",
      "actual_output": "test_case_5: a=2147483647, b=-2147483648"
    },
    {
      "test_id": 6,
      "type": "invalid",
      "inputs": {
        "a": "NULL",
        "b": 5
      },
